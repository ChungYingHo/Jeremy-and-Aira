{
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "astro"
  ],
  "cSpell.words": [
    "Aira",
    "Crect",
    "Csvg",
    "Drilldown",
    "Fira",
    "flexsearch",
    "Frontmatter",
    "grayscale",
    "Hartwell",
    "ithome",
    "Lerp",
    "maxage",
    "Noto",
    "shiki",
    "textable",
    "vocus"
  ]
}
